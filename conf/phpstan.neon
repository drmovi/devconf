includes:
    - ./phpstan-baseline.neon

parameters:

    paths:
         - ../app
         - ../packages

    excludePaths:
        - ../app/vendor
        - ../app/bootstrap/cache
        - ../app/storage

    # Level 9 is the highest level
    level: 5

rules:
    - Drmovi\PackageBoundaries\PackageBoundaries
